{
  "name": "club-escalade",
  "homepage": "https://lopezdav-code.github.io/ALJ_Jonage_Escalade",
  "type": "module",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "start": "vite --host :: --port 3000",
    "dev": "vite --host :: --port 3000",
    "dev:auto": "node scripts/auto-restart.js",
    "build": "vite build",
    "preview": "vite preview --host :: --port 3000",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist --no-history",
    "server:restart": "powershell -ExecutionPolicy Bypass -File scripts/server-manager.ps1 restart",
    "server:status": "powershell -ExecutionPolicy Bypass -File scripts/server-manager.ps1 status",
    "server:start": "powershell -ExecutionPolicy Bypass -File scripts/server-manager.ps1 start",
    "server:stop": "powershell -ExecutionPolicy Bypass -File scripts/server-manager.ps1 stop",
    "server:check": "node -e \"require('http').get('http://localhost:3000/ALJ_Jonage_Escalade/', (res) => console.log('✅ Serveur actif:', res.statusCode)).on('error', () => console.log('❌ Serveur inactif'))\"",
    "update-schema": "node scripts/update-schema.js",
    "test:generate": "node scripts/generate-tests.cjs",
    "test:e2e": "cypress run --headless --browser chrome",
    "test:e2e:ui": "cypress open",
    "test:e2e:debug": "cypress run --headed --browser chrome --no-exit",
    "test:report": "node scripts/merge-reports.cjs && open cypress/reports/mochawesome.html || start cypress/reports/mochawesome.html",
    "test:watch": "cypress run --watch --headless"
  },
  "dependencies": {
